{
  "name": "usermp/laravel-dockerize",
  "description": "Dockerize Laravel applications automatically",
  "require": {
    "php": "^8.3",
    "illuminate/support": "^11.0|^12.0",
    "illuminate/console": "^11.0|^12.0"
  },
  "autoload": {
    "psr-4": {
      "Usermp\\Dockerize\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Usermp\\Dockerize\\DockerizeServiceProvider"
      ]
    }
  }
}
