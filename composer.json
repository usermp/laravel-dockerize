{
  "name": "usermp/laravel-dockerize",
  "description": "Automatically Dockerize Laravel applications with simple artisan commands.",
  "keywords": [
    "laravel",
    "docker",
    "dockerfile",
    "docker-compose",
    "devops",
    "automation"
  ],
  "homepage": "https://github.com/usermp/laravel-dockerize",
  "license": "MIT",
  "authors": [
    {
      "name": "Mohammad",
      "email": "usermp76@gmail.com"
    }
  ],
  "require": {
    "php": "^8.3",
    "illuminate/support": "^11.0|^12.0",
    "illuminate/console": "^11.0|^12.0"
  },
  "autoload": {
    "psr-4": {
      "Usermp\\Dockerize\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Usermp\\Dockerize\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Usermp\\Dockerize\\DockerizeServiceProvider"
      ]
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
